## Indexer-based environment variables
# SubQL / Hasura GraphQL endpoint
SUBQL_ENDPOINT=https://subql.blue.taurus.subspace.network/v1/graphql

# Pagination and batching for counts modes
PAGE_SIZE=1000
CONCURRENCY=6

## Node-based environment variables
CONSENSUS_RPC_URL=wss://rpc.mainnet.autonomys.xyz/ws
DOMAIN_RPC_URL=wss://auto-evm.mainnet.autonomys.xyz/ws
ACK_MODE=dest-only # dest-only | ack-only | both
BLOCK_CONCURRENCY=8
SEGMENT_CONCURRENCY=8
RPC_MAX_RETRIES=5
RPC_BACKOFF_MS=1000

## Common environment variables
# Optional export controls
OUTPUT_DIR=exports

